  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pmagaz/redux-req-middleware/issues"
  },
  "homepage": "https://github.com/pmagaz/redux-req-middleware#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "chai": "^4.0.2",
    "coveralls": "^2.13.1",
    "eslint": "^4.1.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-babel": "^4.1.1",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "redux": "^3.7.1",
    "rimraf": "^2.6.1"
